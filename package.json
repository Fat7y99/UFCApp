    "@reduxjs/toolkit": "^2.9.0",
    "@tanstack/query-async-storage-persister": "^5.90.1",
    "@tanstack/react-query": "^5.90.1",
    "@tanstack/react-query-persist-client": "^5.90.1",
    "axios": "^1.12.2",
