    "@testing-library/react-native": "^13.3.3",
    "@types/i18n-js": "^4.0.1",
    "@types/react": "19.1.0",
    "@types/react-test-renderer": "19.1.0",
    "@types/redux-logger": "^3.0.13",
    "@types/tinycolor2": "^1.4.6",
    "babel-jest": "^30.1.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "8.57.1",
