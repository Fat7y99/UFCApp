    "react": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.81.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-bootsplash": "^6.3.11",
    "react-native-config": "^1.5.9",
    "react-native-device-info": "^14.1.1",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-keyboard-controller": "^1.18.6",
    "react-native-localize": "^3.5.2",
    "react-native-mmkv": "^3.3.3",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-network-logger": "^2.0.1",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-paper": "^5.14.5",
    "react-native-permissions": "^5.4.4",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "^4.1.0",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-uuid": "^2.0.3",
    "react-native-vector-image": "^0.4.5",
    "react-native-worklets": "^0.5.1",
    "react-redux": "^9.2.0",
    "redux-logger": "^3.0.6",
    "rn-local-authentication": "^1.2.5",
    "tinycolor2": "^1.6.0",
    "toastify-react-native": "^7.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
